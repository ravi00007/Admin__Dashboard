    <!-- Main content -->
    <section class="content">
      <div class="container-fluid">
        <% if(details!=null) { %>
        <table  class="table table-bordered">
            <thead>
              <tr>
                <th >Name</th>
                <th >Price</th>
                <th >Brand</th>
                <th >Image</th>
                <th colspan="2">Action</th>
                <th>Status</th>
              </tr>  
            </thead>
            <tbody>
              <% details.forEach(function(item){ %>
              <tr>   
                <td><%= item.name %></td>
                <td><%= item.price %></td>
                <td><%= item.brand %></td>
                <td><img src="<%= item.image %>" style="width: 45px; height: 35px;"></td>
                <td><p><a href="delete/<%= item.id %>" class="btn btn-danger" onclick="return confirm('Are you sure want to Delete');" role="button">Delete</a></p></td>
                <td><p><a href="/sec/<%= item.id %>" class="btn btn-warning"  role="button">Edit</a></p></td>
                
                <% if (item.isactive) { %>
                  <td><p><a href="/isactivatesecond/<%= item.id %>" class="btn btn-info" onclick="return confirm('Are you sure want to Deactivate');"  role="button">Active</a></p></td>
              <% } else { %>
                <td><p><a href="/isactivatesecond/<%= item.id %>" class="btn btn-danger" onclick="return confirm('Are you sure want to Activate');"  role="button">Inative</a></p></td>
              <% } %>
              </tr>
            </tbody>
            <% }) %>
          </table>
          <% } %>
        <div class="row">
          <!-- Left col -->
      
          </section>
          <!-- /.Left col -->
          <!-- right col (We are only adding the ID to make the widgets sortable)-->
          <!-- <section class="col-lg-5 connectedSortable"> -->
          <nav aria-label="Page navigation example">
            <% if (pages > 0) { %>
              <div class="text-xs-center" style="padding-left: 26%; margin-bottom: 5%;">
              <ul class="pagination">
                  <% if (current == 1) { %>
                      <li class=" page-item disabled"><a class="page-link">First</a></li>
                  <% } else { %>
                      <li class="page-item"><a class="page-link" href="/products/1">First</a></li>
                  <% } %>
                  <% var i = (Number(current) > 5 ? Number(current) - 4 : 1) %>
                  <% if (i !== 1) { %>
                      <li class="page-item disabled"><a class="page-link">...</a></li>
                  <% } %>
                  <% for (; i <= (Number(current) + 4) && i <= pages; i++) { %>
                      <% if (i == current) { %>
                          <li class="page-item active"><a class="page-link"><%= i %></a></li>
                      <% } else { %>
                          <li class="page-item"><a class="page-link" href="/products/<%= i %>"><%= i %></a></li>
                      <% } %>
                      <% if (i == Number(current) + 4 && i < pages) { %>
                          <li class=" page-item disabled"><a class="page-item">...</a></li>
                      <% } %>
                  <% } %>
                  <% if (current == pages) { %>
                      <li class="page-item disabled"><a class="page-link">Last</a></li>
                  <% } else { %>
                      <li class="page-item"><a class="page-link" href="/products/<%= pages %>">Last</a></li>
                  <% } %>
              </ul>
              </div>
          <% } %>
        </nav>
          </section>
          <!-- right col -->
        </div>
        <!-- /.row (main row) -->
      </div><!-- /.container-fluid -->

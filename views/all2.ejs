<script>
$(function(){
$('#searchName').autocomplete({
  source: function(req,res){
    $.ajax({
      url:"/autocomplete",
      dataType:"jsonp",
      type:"GET",
      data: req,
      success:function(data){
       res(data)
      },
      error:function(err){
        console.log(err);
      }
    });
  },
  minLength:1,
  select:function(event,ui){
    if(ui.item){
      $( "#searchName" );
      $("#searchName").html(ui.item);
    }
  }
});
});
</script>
